{
    "expo": {
        "name": "LCN Chat",
        "slug": "mobile-social-network",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/iconlogo.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/iconlogo.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.lcn.lcnchat"
        },
        "web": {
            "favicon": "./assets/iconlogo.png"
        },
        "plugins": [
            [
                "expo-image-picker",
                {
                    "photosPermission": "Ứng dụng cần truy cập vào bộ nhớ thiết bị của bạn."
                }
            ]
        ]
    }
}
